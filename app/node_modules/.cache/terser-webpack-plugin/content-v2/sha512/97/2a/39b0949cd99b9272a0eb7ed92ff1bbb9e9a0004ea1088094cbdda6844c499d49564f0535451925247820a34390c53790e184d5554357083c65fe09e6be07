ound.css';\n\nconst NotFound = () => (\n  <div className=\"notfound\">\n    <div className=\"notfound__content\">\n      <h1>404</h1>\n      <h2>OOPS!</h2>\n      <h3>Esta Página No Existe.</h3>\n    </div>\n  </div>\n);\n\nexport default NotFound;\n","import { Switch, Route } from 'react-router-dom';\n\nimport Home from '../components/Home/Home';\nimport Navbar from '../components/Navbar';\nimport NotFound from '../components/NotFound';\n\nconst DashboardRoutes = () => (\n  <>\n    <Navbar />\n    <Switch>\n      <Route exact path=\"/\" component={Home} />\n      <Route component={NotFound} />\n    </Switch>\n  </>\n);\n\nexport default DashboardRoutes;\n","import { Route, Redirect } from 'react-router-dom';\nimport PropTypes from 'prop-types';\n\nconst PublicRoute = ({ component: Component, isLogged, ...args }) => (\n  <Route\n    {...args}\n    component={(props) => (\n      !isLogged\n        ? <Component {...props} />\n        : <Redirect to=\"/\" />\n    )}\n  />\n);\n\nPublicRoute.propTypes = {\n  component: PropTypes.elementType.isRequired,\n  isLogged: PropTypes.bool.isRequired,\n};\n\nexport default PublicRoute;\n","import { Route, Redirect } from 'react-router-dom';\nimport PropTypes from 'prop-types';\n\nconst PrivateRoute = ({ component: Component, isLogged, ...args }) => (\n  <Route\n    {...args}\n    component={(props) => (\n      isLogged\n        ? <Component {...props} />\n        : <Redirect to=\"/login\" />\n    )}\n  />\n);\n\nPrivateRoute.propTypes = {\n  component: PropTypes.elementType.isRequired,\n  isLogged: PropTypes.bool.isRequired,\n};\n\nexport default PrivateRoute;\n","import PropTypes from 'prop-types';\n\nconst AuthenticationMenu = ({ isLogin, changeLoginStatus }) => {\n  const menuClasses = !isLogin ? 'auth__menu left' : 'auth__menu right';\n  return (\n    <div className={menuClasses}>\n      <div className=\"auth__menu-container\">\n        <h1>{ !isLogin ? '¡Bienvenido!' : '¿Aún no tienes una cuenta?' }</h1>\n        <p>\n      